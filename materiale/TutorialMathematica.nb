     RowBox[{"Re", "[", 
      RowBox[{"g", "[", 
       RowBox[{"\[Rho]", ",", "\[Theta]"}], "]"}], "]"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"\[Rho]", ",", "0", ",", "5"}], "}"}], ",", 
   RowBox[{"{", 
    StyleBox[
     RowBox[{"\[Theta]", ",", 
      RowBox[{
       RowBox[{"-", "4"}], "\[Pi]"}], ",", 
      RowBox[{"4", "\[Pi]"}]}],
     FontSize->12], 
    StyleBox[" ",
     FontSize->12], 
    StyleBox["}",
     FontSize->12]}]}], "]"}], "\[IndentingNewLine]", 
